{
  "name": "hdl21-schematics-vscode",
  "private": true,
  "displayName": "Hdl21 Schematics VS Code Plug-In",
  "description": "Hdl21 Schematics VS Code Plug-In",
  "version": "0.0.1",
  "publisher": "dan-fritchman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:hdl21.schematics",
    "onCommand:hdl21.schematics.new"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "hdl21.schematics",
        "displayName": "Hdl21 Schematic",
        "selector": [
          {
            "filenamePattern": "*.sch.svg"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "hdl21.schematics.new",
        "title": "Create new Hdl21 Schematic",
        "category": "Hdl21 Schematics"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc --build && webpack --config webpack.webview.js",
    "watch": "tsc --build && webpack --watch --hot --config webpack.webview.js",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "EditorCore": "^1.0.0",
    "PlatformInterface": "^1.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.65.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
